<?php

namespace MDN\ListBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Item
 * 
 * @ORM\Entity(repositoryClass="MDN\ListBundle\Entity\ItemRepository")
 */
class Item
{
    /**
     * @var integer
     */
    private $itemId;

    /**
     * @var string
     */
    private $name;

    /**
     * @var string
     */
    private $statusId;


    /**
     * Get itemId
     *
     * @return integer 
     */
    public function getItemId()
    {
        return $this->itemId;
    }

    /**
     * Set name
     *
     * @param string $name
     * @return Item
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name
     *
     * @return string 
     */
    public function getName()
    {
        return $this->name;
    }

    /**
     * Set statusId
     *
     * @param string $statusId
     * @return Item
     */
    public function setStatusId($statusId)
    {
        $this->statusId = $statusId;

        return $this;
    }

    /**
     * Get statusId
     *
     * @return string 
     */
    public function getStatusId()
    {
        return $this->statusId;
    }
    /**
     * @var \DateTime
     */
    private $createdAt;

    /**
     * @var \DateTime
     */
    private $updatedAt;


    /**
     * Set createdAt
     *
     * @param \DateTime $createdAt
     * @return Item
     */
    public function setCreatedAt($createdAt)
    {
        $this->createdAt = $createdAt;

        return $this;
    }

    /**
     * Get createdAt
     *
     * @return \DateTime 
     */
    public function getCreatedAt()
    {
        return $this->createdAt;
    }

    /**
     * Set updatedAt
     *
     * @param \DateTime $updatedAt
     * @return Item
     */
    public function setUpdatedAt($updatedAt)
    {
        $this->updatedAt = $updatedAt;

        return $this;
    }

    /**
     * Get updatedAt
     *
     * @return \DateTime 
     */
    public function getUpdatedAt()
    {
        return $this->updatedAt;
    }
    /**
     * @var \MDN\ListBundle\Entity\Priority
     */
    private $priority;


    /**
     * Set priority
     *
     * @param \MDN\ListBundle\Entity\Priority $priority
     * @return Item
     */
    public function setPriority(\MDN\ListBundle\Entity\Priority $priority = null)
    {
        $this->priority = $priority;

        return $this;
    }

    /**
     * Get priority
     *
     * @return \MDN\ListBundle\Entity\Priority 
     */
    public function getPriority()
    {
        return $this->priority;
    }
}
